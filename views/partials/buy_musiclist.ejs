<div class="buy_container">
	<div class="buy_music_container">
		<% for(var i=0; i < lists.length; i++) { %>
        <img src="../../img/<%= img_lists[i] %>" alt="이미지의 묘사 내용" />
		<button type="button" class="buy_music"><%= lists[i] %></button>
		<% }; %>
        <%= img_lists %>
        
	</div>
